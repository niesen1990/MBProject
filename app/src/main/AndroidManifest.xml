        <!-- umeng-analytics -->
        <meta-data
            android:name="UMENG_APPKEY"
            android:value="5538a77367e58e41e0000c09" />
        <meta-data
            android:name="UMENG_MESSAGE_SECRET"
            android:value="86d804b234623b949528183d9ae65ede" />
        <meta-data
